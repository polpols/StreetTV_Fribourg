#frontieres{
  line-color:@frontieres;
  line-join:round;
  line-dasharray:4,4;
  line-smooth:1;
  text-face-name:@font;
  text-fill:@frontieres;
  text-name :'[name]';
  text-size:16;
  text-placement:line;
  text-dy:8;
  
  
  [zoom=14] {line-width:4;}
  [zoom=15] {line-width:5;}
  [zoom=16] {line-width:6;}
  [zoom=17] {line-width:7;}
}
#geo{
line-width:4;
line-color:#bd4949;
line-smooth:1;

}